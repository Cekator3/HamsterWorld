@model List<UserBindingModel>
@{
    if(Model.Count == 0)
    {
        return;
    }
}
@{
    int position = ViewBag.startPosition;
}
<tbody>
@foreach(UserBindingModel user in Model)
{
    @:<tr>
    @:    <th scope="row" class="position">@position</th>
    @:    <th class="login">@user.Login</th>
    @:    <th class="oldRoleName">@user.RoleName</th>
    @:    <th class="newRoleName">@user.RoleName</th>
    @:</tr
    position++;
}

</tbody>
