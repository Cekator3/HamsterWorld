@model CatalogFilter

<input hidden readonly asp-for="filterType">

<div class="row">
	<div class="col-12">
		<label asp-for="Model">Модель</label>
		<input asp-for="Model" class="form-control" placeholder="Intel Core i9">
		<span asp-validation-for="Model" class="text-danger"></span>
	</div>
</div>

<div class="row">
	<div class="col-12">
		<label asp-for="MinPrice">Цена</label>
	</div>
	<div class="col">
		<input asp-for="MinPrice" class="form-control" placeholder="от 0">
	</div>
	<div class="col">
		<input asp-for="MaxPrice" class="form-control" placeholder="до 999999">
	</div>
</div>