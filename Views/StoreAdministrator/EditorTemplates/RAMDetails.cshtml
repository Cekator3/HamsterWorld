@model RAMDetails

<div class="form-group mb-2">
	<label asp-for="MemoryType">Тип памяти</label>
	<input asp-for="MemoryType" class="form-control">
	<span asp-validation-for="MemoryType" class="text-danger"></span>
</div>

<div class="form-group mb-2">
	<label asp-for="AmountOfMemory">Количество памяти</label>
	<input asp-for="AmountOfMemory" class="form-control">
	<span asp-validation-for="AmountOfMemory" class="text-danger"></span>
</div>