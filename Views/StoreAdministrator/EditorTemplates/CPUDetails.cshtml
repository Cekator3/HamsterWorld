@model CPUDetails

<div class="form-group mb-2">
	<label asp-for="Socket">Сокет</label>
	<input asp-for="Socket" class="form-control" placeholder="Сокет">
	<span asp-validation-for="Socket" class="text-danger"></span>
</div>

<div class="form-group mb-2">
	<label asp-for="NumberOfCores">Количество ядер</label>
	<input asp-for="NumberOfCores" class="form-control" placeholder="16">
	<span asp-validation-for="Socket" class="text-danger"></span>
</div>

<div class="form-group mb-2">
	<label asp-for="ClockRate">Тактовая частота</label>
	<input asp-for="ClockRate" class="form-control" placeholder="32000">
	<span asp-validation-for="Socket" class="text-danger"></span>
</div>