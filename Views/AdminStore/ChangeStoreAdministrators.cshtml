@model List<ChangeStoreAdministratorsBindingModel>
<div id="xhrAnswer"></div>

<table id="Users" class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Логин</th>
            <th scop="col">Email</th>
            <th scope="col">Администратор</th>
        </tr>
    </thead>

    <tbody>
        @for (byte i = 0; i < Model.Count; i++)
        {
            <tr>
                <th scope="row">@(i + 1)</th>
                <th scope="row">@Model[i].Login</th>
                <th scope="row">@Model[i].Email</th>
                <th><input class="IsAdminOfThisStore" type="checkbox" value="@Model[i].IsAdminOfThisStore" storeId="@ViewBag.StoreId" adminId="@Model[i].AdminId"></th>
            </tr>
        }
    </tbody>
</table>

<script src="~/js/AdminStore/ChangeStoreAdmins.js"></script>